<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vip.vipagents">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_icon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.tools.EventActivity"
            android:label="@string/title_activity_event"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".ui.tools.WrtieEventActivity" />
        <activity android:name=".ui.slideshow.MissionActivity" />
        <activity android:name=".EditPasswordActivity" />
        <activity android:name=".ProfileEditActivity" />
        <activity
            android:name=".MyPageActivity"
            android:label="@string/title_activity_my_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ui.share.WriteNoticeActivity" />
        <activity
            android:name=".ui.share.NoticeActivity"
            android:label="@string/title_activity_notice"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignupActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>